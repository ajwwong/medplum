{
  "name": "medplum-chart-demo",
  "version": "4.3.11",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:bots && tsc && vite build",
    "build:bots": "npm run clean && tsc --project tsconfig-bots.json && node --no-warnings esbuild-script.mjs && node --loader ts-node/esm src/scripts/deploy-bots.ts",
    "clean": "rimraf dist",
    "dev": "vite",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "@medplum/eslint-config"
    ]
  },
  "devDependencies": {
    "@mantine/core": "7.17.8",
    "@mantine/hooks": "7.17.8",
    "@mantine/notifications": "7.17.8",
    "@medplum/bot-layer": "4.3.11",
    "@medplum/core": "4.3.11",
    "@medplum/definitions": "4.3.11",
    "@medplum/eslint-config": "4.3.11",
    "@medplum/fhirtypes": "4.3.11",
    "@medplum/mock": "4.3.11",
    "@medplum/react": "4.3.11",
    "@tabler/icons-react": "3.36.0",
    "@types/node": "20.19.27",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "chart.js": "4.5.1",
    "esbuild": "0.27.1",
    "fast-glob": "3.3.3",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "react": "19.2.3",
    "react-chartjs-2": "5.3.1",
    "react-dom": "19.2.3",
    "react-router": "7.10.1",
    "rimraf": "6.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vitest": "3.2.4"
  },
  "packageManager": "npm@10.9.4",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
